	task.wait() 
	if (r:IsA(SLlrVimgQfjCkhABozWcDvbZOSdppBPgvGjbBBtwjBcLYykGfdSktviIPnIiyhmsfSJcMW('UtjRxoRptAkTRDYhJbIIFUNtvYLjMYcvoIVOZkKBKtaVFKuzeshmLnMUmVtb3RlRXZlbnQ=')) or r:IsA(SLlrVimgQfjCkhABozWcDvbZOSdppBPgvGjbBBtwjBcLYykGfdSktviIPnIiyhmsfSJcMW('djMfcYQpuYUtFbFGwbxgHTFmeoPMFnmnITpxAGXLGIdOHXqMBzlgShiUmVtb3RlRnVuY3Rpb24='))) and r.Name:lower():find(SLlrVimgQfjCkhABozWcDvbZOSdppBPgvGjbBBtwjBcLYykGfdSktviIPnIiyhmsfSJcMW('zuuCRGThqIxauCXHyBspVNKTFVHcGytYtWzQdSMCHQSWUZyonlannDeZ2lmdA==')) then
		AntiGift:Log(SLlrVimgQfjCkhABozWcDvbZOSdppBPgvGjbBBtwjBcLYykGfdSktviIPnIiyhmsfSJcMW('CptIjUNybcWnjLbliSWVEUFwpMXPvFohHvEQdMJDPZTANpproWiExcYRGV0ZWN0ZWQ='), r.Name, Color3.fromRGB(255, 255, 0), SLlrVimgQfjCkhABozWcDvbZOSdppBPgvGjbBBtwjBcLYykGfdSktviIPnIiyhmsfSJcMW('eGtIijyxvKShPhIlAysYSnpFiWVpIitMaxjMwgXxUBNblVksBpsHVgsRHluYW1pY2FsbHkgZm91bmQgbmV3IHJlbW90ZS4='))
		AntiGift:BlockGiftRemote(r)
		AntiGift:PlaySound(SLlrVimgQfjCkhABozWcDvbZOSdppBPgvGjbBBtwjBcLYykGfdSktviIPnIiyhmsfSJcMW('WuyCtblBFaFSUpnjTXLHFqzhKtdKwJnwthMavLfXsjVieMVJMgHaTbvMjYzMjM0MjIy'), 0.6, 1.2) -- Notification sound
	end
end)

