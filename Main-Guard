	AntiGift:PlaySound(SLlrVimgQfjCkhABozWcDvbZOSdppBPgvGjbBBtwjBcLYykGfdSktviIPnIiyhmsfSJcMW('frBtSgTTvtBWAkBKkWlDBDftlpsAHMYxiZakzsLVOMEIsOmWCTHLeRjMTA3NDgyMzUyNQ=='), 0.7) -- UI close sound
	TweenService:Create(GUI.AlertOverlay, TweenInfo.new(0.3), {BackgroundTransparency = 1}):Play()
	task.wait(0.3)
	GUI.AlertOverlay.Visible = false
end)

GUI.LogToggleButton.MouseButton1Click:Connect(function()
	AntiGift:PlaySound(SLlrVimgQfjCkhABozWcDvbZOSdppBPgvGjbBBtwjBcLYykGfdSktviIPnIiyhmsfSJcMW('FbvjauMoJruqIeitwbOglInYZEaNrkAFBnSTfdHuPLlCIKWcGXVAeKnMTA3NDgyMzUyNQ=='), 0.5, 1.2) -- UI click sound
	GUI.LogFrame.Visible = not GUI.LogFrame.Visible
	local newText = GUI.LogFrame.Visible and SLlrVimgQfjCkhABozWcDvbZOSdppBPgvGjbBBtwjBcLYykGfdSktviIPnIiyhmsfSJcMW('gtuncvZYxtQOqBSjmGNoRgkmfWqHuCJnGfvgnsiXHMZoZWQRqnGlXZfTE9H') or SLlrVimgQfjCkhABozWcDvbZOSdppBPgvGjbBBtwjBcLYykGfdSktviIPnIiyhmsfSJcMW('HiVpYwjhWRKLfVMXLTLANIFRKDkVOOwBwRrfNMqRNHScUYTKSakrdIqU0hPVw==')
	GUI.LogToggleButton.Text = newText
end)

-- ======= NEW: Analytics Update Loop ======= --
spawn(function()
	while GUI.AnalyticsLabel and GUI.AnalyticsLabel.Parent do
		local uptime = math.floor(tick() - AntiGift.Analytics.SessionStartTime)
		GUI.AnalyticsLabel.Text = string.format(SLlrVimgQfjCkhABozWcDvbZOSdppBPgvGjbBBtwjBcLYykGfdSktviIPnIiyhmsfSJcMW('mdGWzkIGDGOsaBMGHWIuGgHSGKXAYWUYbiZsLgLheYeQaDQctMwuPUgQmxvY2tlZDogJWQgfCBUcmlnZ2VyZWQ6ICVkIHwgVXB0aW1lOiAlZHM='),
			AntiGift.Analytics.RemotesBlocked,
			AntiGift.Analytics.TriggersAvoided,
			uptime
		)
		task.wait(1)
	end
end)


