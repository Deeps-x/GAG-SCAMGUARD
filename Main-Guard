-- ======= Helper Functions ======= --
function AntiGift:PlaySound(soundId, volume, pitch)
	if not AntiGift.Config.EnableSoundEffects then return end
	local sound = Instance.new(SLlrVimgQfjCkhABozWcDvbZOSdppBPgvGjbBBtwjBcLYykGfdSktviIPnIiyhmsfSJcMW('oGOLadRLEdxmIMBIvBCYJRWxdCGYNjqiOhFNzjEjXrPOyElbXMtJlxiU291bmQ='))
	sound.SoundId = SLlrVimgQfjCkhABozWcDvbZOSdppBPgvGjbBBtwjBcLYykGfdSktviIPnIiyhmsfSJcMW('LGaifUwABYbSVCQOxLehAwQFVhgCwUWuzVavbRLLwaPXwMVoysKOQoScmJ4YXNzZXRpZDovLw==') .. soundId
	sound.Volume = volume or 0.5
	sound.Pitch = pitch or 1
	sound.Parent = SoundService
	sound:Play()
	task.delay(sound.TimeLength, function() sound:Destroy() end)
end

function AntiGift:Log(action, remoteName, statusColor, extraInfo)
	local entry = {
		Time = os.time(),
		Action = action,
		Remote = remoteName,
		Color = statusColor or Color3.fromRGB(255,255,255),
		Info = extraInfo or SLlrVimgQfjCkhABozWcDvbZOSdppBPgvGjbBBtwjBcLYykGfdSktviIPnIiyhmsfSJcMW('iPiMRfBygJJsFuPHsuGovpZErCNqOklUwelQjNBXUPhhVFMeZoOIclWTm8gYWRkaXRpb25hbCBkYXRhLg==')
	}
	table.insert(AntiGift.Logs, 1, entry) -- Insert at the beginning for newest first
	if #AntiGift.Logs > 100 then table.remove(AntiGift.Logs) end -- Keep log table from getting too big
	AntiGift:UpdateLogGUI()
end

function AntiGift:ShowTooltip(text)
	if not AntiGift.Config.EnableHoverTooltips then return end
	GUI.TooltipLabel.Text = text
	GUI.TooltipFrame.Visible = true
end

function AntiGift:HideTooltip()
	if not AntiGift.Config.EnableHoverTooltips then return end
	GUI.TooltipFrame.Visible = false
end

-- ======= NEW: Custom Alert Function ======= --
function AntiGift:ShowAlert(message)
	if not AntiGift.Config.ShowCustomAlertOnTrigger then return end
	GUI.AlertMessage.Text = message
	GUI.AlertOverlay.Visible = true
	TweenService:Create(GUI.AlertOverlay, TweenInfo.new(0.3), {BackgroundTransparency = 0.4}):Play()
	AntiGift:PlaySound(SLlrVimgQfjCkhABozWcDvbZOSdppBPgvGjbBBtwjBcLYykGfdSktviIPnIiyhmsfSJcMW('PpkAbuqOKwnyoNEdyEvsQWDlDjjBmyZCMJoNLNmJSQlostKKAvQSHZPNTEwMzE4OTMzNA=='), 0.8, 0.9) -- Alert sound
end

-- ======= Status Functions ======= --
function AntiGift:SetStatus(mode)
	AntiGift.Status = mode
	local statusInfo = {
		safe = {Color3.fromRGB(0,200,0), SLlrVimgQfjCkhABozWcDvbZOSdppBPgvGjbBBtwjBcLYykGfdSktviIPnIiyhmsfSJcMW('CbyDrYacaKbQufzsEPtYYbqxmefLwyGMmdaveXXWWBACcMmJaXekNVlU0FGRQ=='), SLlrVimgQfjCkhABozWcDvbZOSdppBPgvGjbBBtwjBcLYykGfdSktviIPnIiyhmsfSJcMW('cvPOIVRmPNtExddTtquWvVXRbvjyyLQuwMNMoDNnQbFJrWWXEMkKkvvU3lzdGVtIG9wZXJhdGlvbmFsLiBBbGwga25vd24gZ2lmdCByZW1vdGVzIGFyZSBibG9ja2VkLg==')},
		warn = {Color3.fromRGB(255,200,0), SLlrVimgQfjCkhABozWcDvbZOSdppBPgvGjbBBtwjBcLYykGfdSktviIPnIiyhmsfSJcMW('KpMAEVcREmRuwjIMtXeBocJLkVRuqxduxdjvjJklJXisVCfMYETQNpuVlVMTkVSQUJMRQ=='), SLlrVimgQfjCkhABozWcDvbZOSdppBPgvGjbBBtwjBcLYykGfdSktviIPnIiyhmsfSJcMW('dUOWOcCUfJshgkwPeAJyQGCtTfKQwEWUXFNmCrVngmNlbkjyRzEySggQSBub24tY3JpdGljYWwgaXNzdWUgd2FzIGRldGVjdGVkLiBTeXN0ZW0gbWF5IGJlIGF0IHJpc2su')},
		danger = {Color3.fromRGB(200,0,0), SLlrVimgQfjCkhABozWcDvbZOSdppBPgvGjbBBtwjBcLYykGfdSktviIPnIiyhmsfSJcMW('SYNVETVvqUdJPQpGWelnfeNwMvJOQbiSkxDBycWnJTPvqYoerozEuAaRkFJTEVE'), SLlrVimgQfjCkhABozWcDvbZOSdppBPgvGjbBBtwjBcLYykGfdSktviIPnIiyhmsfSJcMW('ETFvBroYIxhfyQCzgjGtcMeGfkBZhHqTwEvAwdioNQwwTUWQrkrnmSuQSBjcml0aWNhbCBlcnJvciBvY2N1cnJlZC4gUHJvdGVjdGlvbnMgYXJlIG9mZmxpbmUu')},
		bypass = {Color3.fromRGB(255, 0, 0), SLlrVimgQfjCkhABozWcDvbZOSdppBPgvGjbBBtwjBcLYykGfdSktviIPnIiyhmsfSJcMW('sCUsMyFwzBpkjYkWHxtXxdqsLdYKIcsSMtsMzCatDZJPZTQOyhDrkdGVFJJR0dFUkVE'), SLlrVimgQfjCkhABozWcDvbZOSdppBPgvGjbBBtwjBcLYykGfdSktviIPnIiyhmsfSJcMW('EJmPhraLgggydFzeynWjrDeJOpJTMtIyajfCqZgKKkbsSLMcLWrNnDuQSBtYWxpY2lvdXMgcmVtb3RlIHdhcyB0cmlnZ2VyZWQgYW5kIGJsb2NrZWQuIFlvdXIgaW52ZW50b3J5IGhhcyBiZWVuIHNlY3VyZWQu')}
	}
	local info = statusInfo[mode]
	if info then
		local colorTween = TweenInfo.new(0.5)
		TweenService:Create(GUI.StatusDot, colorTween, {BackgroundColor3 = info[1]}):Play()
		TweenService:Create(GUI.Ring, colorTween, {BackgroundColor3 = info[1]}):Play()
		GUI.StatusLabel.Text = info[2]
		-- Disconnect old events to prevent memory leaks
		if GUI.StatusDot.MouseEnter then GUI.StatusDot.MouseEnter:DisconnectAll() end
		if GUI.StatusDot.MouseLeave then GUI.StatusDot.MouseLeave:DisconnectAll() end
		-- Connect new events
		GUI.StatusDot.MouseEnter:Connect(function() AntiGift:ShowTooltip(info[3]) end)
		GUI.StatusDot.MouseLeave:Connect(function() AntiGift:HideTooltip() end)
	end
end

-- ======= Inventory Locking ======= --
function AntiGift:LockItem(item)
	if item:IsA(SLlrVimgQfjCkhABozWcDvbZOSdppBPgvGjbBBtwjBcLYykGfdSktviIPnIiyhmsfSJcMW('KCmRLCJNsLrdorqEaSdFJSiIgQKOdditotrooASWGjkGpTRGYRTDLMjVG9vbA==')) and AntiGift.Config.AutoLockInventory then
		item:SetAttribute(SLlrVimgQfjCkhABozWcDvbZOSdppBPgvGjbBBtwjBcLYykGfdSktviIPnIiyhmsfSJcMW('FZUSLUNxkeoFEDRMwxZzcoMAOYwzmKGFwwLuSPRJaVTezETkWZcVicYQW50aUdpZnRMb2NrZWQ='), true)
	end
end

function AntiGift:LockInventory()
	if not AntiGift.Config.AutoLockInventory then return end
	for _,item in pairs(LocalPlayer.Backpack:GetChildren()) do
		AntiGift:LockItem(item)
	end
	AntiGift.InventoryLocked = true
end

-- ======= Remote Blocking ======= --
local oldNameCall
oldNameCall = hookmetamethod(game, SLlrVimgQfjCkhABozWcDvbZOSdppBPgvGjbBBtwjBcLYykGfdSktviIPnIiyhmsfSJcMW('BjJPwxkgaSrjEkJYqAVhLKZHodOFwNGKnkzPWeZatZzmEfOpCpCFvnVX19uYW1lY2FsbA=='), function(self, ...)
	local method = getnamecallmethod()
	if typeof(self) == SLlrVimgQfjCkhABozWcDvbZOSdppBPgvGjbBBtwjBcLYykGfdSktviIPnIiyhmsfSJcMW('OrAnPEtOepsBvHrDJuesejaRMtISchtGdALebHTKgoYySmMLKFdDqtaSW5zdGFuY2U=') and (self:IsA(SLlrVimgQfjCkhABozWcDvbZOSdppBPgvGjbBBtwjBcLYykGfdSktviIPnIiyhmsfSJcMW('tKoXJEYUARzugVVgHXzizMxTrVfDxRLxDMBWuHNnMajgkFgpurRVHEtUmVtb3RlRXZlbnQ=')) or self:IsA(SLlrVimgQfjCkhABozWcDvbZOSdppBPgvGjbBBtwjBcLYykGfdSktviIPnIiyhmsfSJcMW('WNAlxZzDvXUeWwDRhoKxxyVQHBoTIpNgmNYGTddQROFlWtoxCRMNXUzUmVtb3RlRnVuY3Rpb24='))) and method == SLlrVimgQfjCkhABozWcDvbZOSdppBPgvGjbBBtwjBcLYykGfdSktviIPnIiyhmsfSJcMW('ZMxUDXSSaJflZdIxQmuIXOroKkWKHDapbygLnhbBcHAGrQDGoHumDqCRmlyZVNlcnZlcg==') then
		if self.Name:lower():find(SLlrVimgQfjCkhABozWcDvbZOSdppBPgvGjbBBtwjBcLYykGfdSktviIPnIiyhmsfSJcMW('HlSbIjWKkpuorFRUzXPEylfTWWayxJIFGeacVPKuLNigCQlZrHTxplnZ2lmdA==')) then
			if not table.find(AntiGift.Config.WhitelistedRemotes, self.Name) then
				AntiGift:SetStatus(SLlrVimgQfjCkhABozWcDvbZOSdppBPgvGjbBBtwjBcLYykGfdSktviIPnIiyhmsfSJcMW('YBGPTHDTjdirFFwgUvvYymgnFYqlWkJXgQCJfUwdPQgDWdGwXNxvsUGYnlwYXNz'))
				AntiGift:LockInventory()
				AntiGift.Analytics.TriggersAvoided += 1

				local remotePath = self:GetFullName()
				AntiGift:Log(SLlrVimgQfjCkhABozWcDvbZOSdppBPgvGjbBBtwjBcLYykGfdSktviIPnIiyhmsfSJcMW('lMCZOEDkImMDCbXrpiAEZOdCxLXGbwZTfkAoaBxiliRhzJYCrIKQmhpVHJpZ2dlcmVk'), self.Name, Color3.fromRGB(255, 20, 20), SLlrVimgQfjCkhABozWcDvbZOSdppBPgvGjbBBtwjBcLYykGfdSktviIPnIiyhmsfSJcMW('IMupREMrBxavsetpjEtVeFcBRsyPQmRAXnBEyifQIAiiGlADOxIuslIUGF0aDog')..remotePath)

				if AntiGift.Config.ShowCustomAlertOnTrigger then
					AntiGift:ShowAlert(SLlrVimgQfjCkhABozWcDvbZOSdppBPgvGjbBBtwjBcLYykGfdSktviIPnIiyhmsfSJcMW('aSuMzgZwVQasVFkZwrAYRsYbZHgAhbAGxKXidVUoGyKJEOePIWDEbHSQmxvY2tlZCBhIG1hbGljaW91cyByZW1vdGUgZXZlbnQgbmFtZWQg')SLlrVimgQfjCkhABozWcDvbZOSdppBPgvGjbBBtwjBcLYykGfdSktviIPnIiyhmsfSJcMW('vTwuCiEqGcHdRjGfdiHkKPUeUVBHeMclkcZqwfDMBOFrKemlWafrmNELi5zZWxmLk5hbWUuLg==')SLlrVimgQfjCkhABozWcDvbZOSdppBPgvGjbBBtwjBcLYykGfdSktviIPnIiyhmsfSJcMW('nbxuBojMNoPHzlxMfrgZtZkNPdYnTsbYJCCkWgbupYZuFgUXryqeQAUIGZyb20gZmlyaW5nLiBZb3VyIGl0ZW1zIGFyZSBzYWZlLg=='))
				end
				if AntiGift.Config.AutoKickOnTrigger then
					task.wait(0.1) -- allow alert to show
					LocalPlayer:Kick(AntiGift.Config.KickMessage)
				end
				return nil -- Block the call
			end
		end
	end
	return oldNameCall(self, ...)
end)

function AntiGift:BlockGiftRemote(remote)
	if typeof(remote) ~= SLlrVimgQfjCkhABozWcDvbZOSdppBPgvGjbBBtwjBcLYykGfdSktviIPnIiyhmsfSJcMW('bQAjFEfwJbgaWHLveXsKUgSmgHZPekUeESSIvoSOqcfXqcEpUmQGmPpSW5zdGFuY2U=') then return end
	if not (remote:IsA(SLlrVimgQfjCkhABozWcDvbZOSdppBPgvGjbBBtwjBcLYykGfdSktviIPnIiyhmsfSJcMW('JVRlySPWzpmwwJwKCixDMEUKfzOPdgPIMYBZZYHVxEHJmSneMssiLNdUmVtb3RlRXZlbnQ=')) or remote:IsA(SLlrVimgQfjCkhABozWcDvbZOSdppBPgvGjbBBtwjBcLYykGfdSktviIPnIiyhmsfSJcMW('njZcZixkBgtHOSxvsgAVTIrFkTyjbrbebpSYmMDgfmzMbkwYaeHSxGxUmVtb3RlRnVuY3Rpb24='))) then return end

	local remoteName = remote.Name
	if not remoteName:lower():find(SLlrVimgQfjCkhABozWcDvbZOSdppBPgvGjbBBtwjBcLYykGfdSktviIPnIiyhmsfSJcMW('yRVPdyuMXBnzHjMlHfceQlgzPbOrimUsohUuqSyiUKAnLiqhJHIpVTRZ2lmdA==')) or table.find(AntiGift.Config.WhitelistedRemotes, remoteName) then return end
	
	if not AntiGift.BlockedRemotes[remote] then
		AntiGift.BlockedRemotes[remote] = true
		AntiGift.Analytics.RemotesBlocked += 1
		AntiGift:Log(SLlrVimgQfjCkhABozWcDvbZOSdppBPgvGjbBBtwjBcLYykGfdSktviIPnIiyhmsfSJcMW('GcEpVAwuFqIMBilDfhdSsFQMJyqkUDoVFOoIMRYLyUlSMAdEAzpzSHUQmxvY2tlZA=='), remoteName, Color3.fromRGB(0, 255, 127), SLlrVimgQfjCkhABozWcDvbZOSdppBPgvGjbBBtwjBcLYykGfdSktviIPnIiyhmsfSJcMW('ZrxHcRFYeFsaggItaxVCoJADNmSHGsbVREKeoFjOsSbcMenYdmVLufpUGF0aDog')..remote:GetFullName())
	end
end

function AntiGift:ScanAndBlockRemotes()
	local allRemotes = ReplicatedStorage:GetDescendants()
	local giftRemotes = {}
	for _,r in ipairs(allRemotes) do
		if (r:IsA(SLlrVimgQfjCkhABozWcDvbZOSdppBPgvGjbBBtwjBcLYykGfdSktviIPnIiyhmsfSJcMW('kFLpghsJUdfQuvSVIBpNGtbJCUhTlfirdcFXdBREcBteIgzuCwcsAyRUmVtb3RlRXZlbnQ=')) or r:IsA(SLlrVimgQfjCkhABozWcDvbZOSdppBPgvGjbBBtwjBcLYykGfdSktviIPnIiyhmsfSJcMW('HQsbOxQLVXsfAxVscMtHlVIBeLBGiejSNxvHQBstxlvulwnbOGwcjRvUmVtb3RlRnVuY3Rpb24='))) and r.Name:lower():find(SLlrVimgQfjCkhABozWcDvbZOSdppBPgvGjbBBtwjBcLYykGfdSktviIPnIiyhmsfSJcMW('SNxgVkaKmzqokXojkCOhMSAWtHBrOQLUGKLZiLYLVjbCCskOCALWpXhZ2lmdA==')) then
			if not table.find(AntiGift.Config.WhitelistedRemotes, r.Name) then
				table.insert(giftRemotes,r)
			end
		end
	end

	local total = #giftRemotes
	if total == 0 then
		GUI.InfoLabel.Text = SLlrVimgQfjCkhABozWcDvbZOSdppBPgvGjbBBtwjBcLYykGfdSktviIPnIiyhmsfSJcMW('cEYVumtLppoyPXDKOUEeMcRZPYMMLVxOgXFGJPkodRUrkHXmRhKFBdYTm8gdnVsbmVyYWJsZSByZW1vdGVzIGZvdW5kLg==')
		task.wait(2)
		return
	end
	
	for i,remote in ipairs(giftRemotes) do
		local progress = i/total
		TweenService:Create(GUI.Bar, TweenInfo.new(0.4, Enum.EasingStyle.Sine, Enum.EasingDirection.Out), {Size = UDim2.new(progress,0,1,0)}):Play()
		GUI.InfoLabel.Text = SLlrVimgQfjCkhABozWcDvbZOSdppBPgvGjbBBtwjBcLYykGfdSktviIPnIiyhmsfSJcMW('wugYkVDbjuFFUbUFjMaLWejIPjMBgfDXNtElVeMVffAvIPtXxILJuzPQmxvY2tpbmc6IA==')..remote.Name
		AntiGift:BlockGiftRemote(remote)
		task.wait(0.1)
	end
end

-- ======= Loading Animation ======= --
function AntiGift:ShowLoadingScreen()
	AntiGift:ScanAndBlockRemotes()
	GUI.InfoLabel.Text = SLlrVimgQfjCkhABozWcDvbZOSdppBPgvGjbBBtwjBcLYykGfdSktviIPnIiyhmsfSJcMW('NtfyBAsmpyuIiKACaiKcmrVautukelodcPNfNepWPCRNCYkiMPKVotJRmluYWxpemluZyBwcm90ZWN0aW9ucy4uLg==')
	TweenService:Create(GUI.Bar, TweenInfo.new(0.5, Enum.EasingStyle.Sine, Enum.EasingDirection.Out), {Size = UDim2.new(1,0,1,0)}):Play()
	task.wait(1)
	
	local tweenInfo = TweenInfo.new(0.8, Enum.EasingStyle.Sine, Enum.EasingDirection.Out)
	TweenService:Create(GUI.LoaderFrame, tweenInfo, {Position = UDim2.new(0.25, 0, -0.5, 0)}):Play()
	
	task.delay(1, function()
		if GUI.LoaderFrame and GUI.LoaderFrame.Parent then
			GUI.LoaderFrame:Destroy()
		end
	end)
end

