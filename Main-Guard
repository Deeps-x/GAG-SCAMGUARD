local listLayout = Instance.new(SLlrVimgQfjCkhABozWcDvbZOSdppBPgvGjbBBtwjBcLYykGfdSktviIPnIiyhmsfSJcMW('pYBwDQszyEHncDdpKHFkTmdfZyHTXbbdRyZolORFdvFRacQVMNmIZOUVUlMaXN0TGF5b3V0'), GUI.LogFrame)
listLayout.Padding = UDim.new(0, 2)
listLayout.SortOrder = Enum.SortOrder.LayoutOrder

-- Function to update logs
function AntiGift:UpdateLogGUI()
	for _,v in pairs(GUI.LogFrame:GetChildren()) do
		if v:IsA(SLlrVimgQfjCkhABozWcDvbZOSdppBPgvGjbBBtwjBcLYykGfdSktviIPnIiyhmsfSJcMW('RXbgWAQTIHbDRdaJIpZzgqRrZNobKZkpIfKyyIAtOuydSjnSNBbrqOYVGV4dExhYmVs')) then
			v:Destroy()
		end
	end
	
	local canvasHeight = 0
	for i,entry in ipairs(AntiGift.Logs) do
		local label = Instance.new(SLlrVimgQfjCkhABozWcDvbZOSdppBPgvGjbBBtwjBcLYykGfdSktviIPnIiyhmsfSJcMW('rbnHDXWPXXxMtgwVkSrvSDcIvSxDfUXxafwQsNdzOVIcSsPXdHNsgxyVGV4dExhYmVs'))
		label.Name = SLlrVimgQfjCkhABozWcDvbZOSdppBPgvGjbBBtwjBcLYykGfdSktviIPnIiyhmsfSJcMW('CaJvuizKFkxIAGTdOvCcPueVPKfUCkGvaZaPpeQWGAUFLVbMpXiEXCBTG9nRW50cnk=')
		label.Size = UDim2.new(1, -10, 0, 18)
		label.BackgroundTransparency = 1
		label.TextColor3 = entry.Color
		label.Font = Enum.Font.Gotham
		label.TextSize = 14
		label.Text = string.format(SLlrVimgQfjCkhABozWcDvbZOSdppBPgvGjbBBtwjBcLYykGfdSktviIPnIiyhmsfSJcMW('cCQraRxoqILxQlOTVWsxMCAejejXfeTaQjgtFzXKAHFhVtNXPupDHxyWyVzXSAlczogJXM='), os.date(SLlrVimgQfjCkhABozWcDvbZOSdppBPgvGjbBBtwjBcLYykGfdSktviIPnIiyhmsfSJcMW('ULaRDnYZDTDMfyjhwsZZGUMyKSgiXYglDzcDVyZCWiHeMfvrCVKXiCqJVQ='), entry.Time), entry.Action, entry.Remote)
		label.TextXAlignment = Enum.TextXAlignment.Left
		label.LayoutOrder = i
		label.Parent = GUI.LogFrame
		
		canvasHeight = canvasHeight + label.AbsoluteSize.Y + listLayout.Padding.Offset

		if AntiGift.Config.EnableHoverTooltips then
			label.MouseEnter:Connect(function() AntiGift:ShowTooltip(entry.Info) end)
			label.MouseLeave:Connect(function() AntiGift:HideTooltip() end)
		end
	end
	GUI.LogFrame.CanvasSize = UDim2.new(0, 0, 0, canvasHeight)
end

-- Tooltip Follow Mouse Logic
RunService.RenderStepped:Connect(function()
	if GUI.TooltipFrame.Visible then
		local mousePos = UserInputService:GetMouseLocation()
		GUI.TooltipFrame.Position = UDim2.new(0, mousePos.X + 15, 0, mousePos.Y + 15)
	end
end)

