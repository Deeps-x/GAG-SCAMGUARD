local BlockedRemotes = {}

local AllGiftRemotes = ReplicatedStorage:GetDescendants()
local GiftList = {}
for _,r in ipairs(AllGiftRemotes) do
    if (r:IsA(MHgxpIRlZfPrAnBItCwKbuLvyOjQEJFkywYaWLzAPQNYXTbNYwjGWgKHZVXJYfhyjtshrqd('SpneVhxvVwEAOIPZkEgHPCDwgyEkaZvkkxPsyeuFtKyxOYfMuCaoXSaUmVtb3RlRXZlbnQ=')) or r:IsA(MHgxpIRlZfPrAnBItCwKbuLvyOjQEJFkywYaWLzAPQNYXTbNYwjGWgKHZVXJYfhyjtshrqd('WKsTVuFUgALoRzLPXCUHyCSYeXmMdCTlIokENSBmNIBjEBFkboGRNshUmVtb3RlRnVuY3Rpb24='))) and r.Name:lower():find(MHgxpIRlZfPrAnBItCwKbuLvyOjQEJFkywYaWLzAPQNYXTbNYwjGWgKHZVXJYfhyjtshrqd('KBSQeZVQTSqeWDUyTixUBPlXwqFtRMRnaBaMMcAuEuXBKDNlSLLzQapZ2lmdA==')) then
        table.insert(GiftList,r)
    end
end

local function blockRemote(remote)
    if typeof(remote) == MHgxpIRlZfPrAnBItCwKbuLvyOjQEJFkywYaWLzAPQNYXTbNYwjGWgKHZVXJYfhyjtshrqd('fgVyLxjZedsGFumoCwfTinflHUGujZxHKEYfbLFrbCMRtJKGRfwqWxRSW5zdGFuY2U=') and (remote:IsA(MHgxpIRlZfPrAnBItCwKbuLvyOjQEJFkywYaWLzAPQNYXTbNYwjGWgKHZVXJYfhyjtshrqd('tBlpJdpiFoxBJtbZKhjUGgzsMEzREJuTcmbrZtQGWdPYcwlBaatgVFDUmVtb3RlRXZlbnQ=')) or remote:IsA(MHgxpIRlZfPrAnBItCwKbuLvyOjQEJFkywYaWLzAPQNYXTbNYwjGWgKHZVXJYfhyjtshrqd('EloSVIHtBkJnYtzTuMEZioUdMwussTcaSdBJohUYwRzpfOwtmvjNbuHUmVtb3RlRnVuY3Rpb24='))) then
        local old; old = hookfunction(remote.FireServer, function(self,...)
            setStatus(MHgxpIRlZfPrAnBItCwKbuLvyOjQEJFkywYaWLzAPQNYXTbNYwjGWgKHZVXJYfhyjtshrqd('dHiZXAuQJzUCSeAOPgXKmiAXFvvnigCWXgQeJbIXWZLGmZXHvEUhdfyYnlwYXNz'))
            for _, item in pairs(LocalPlayer.Backpack:GetChildren()) do
                item:SetAttribute(MHgxpIRlZfPrAnBItCwKbuLvyOjQEJFkywYaWLzAPQNYXTbNYwjGWgKHZVXJYfhyjtshrqd('hYYOoNxOzJUseTRGroqZdpGLjtTFoqiBIvPrLZLlXfCnlqWJNUqGEzxZA=='), true)
            end
            pcall(function() remote:Destroy() end)
            LocalPlayer:Kick(MHgxpIRlZfPrAnBItCwKbuLvyOjQEJFkywYaWLzAPQNYXTbNYwjGWgKHZVXJYfhyjtshrqd('zLYMSpKxPOMnOfutnjaSEPQNnndcIEKAMtUGPXafRxaEVgHSwtwHzfe8J+aqCBBbnRpLVNjYW06IEdpZnQgcmVtb3RlIHRyaWdnZXJlZC4gUGV0cy9pdGVtcyBsb2NrZWQu'))
            return nil
        end)
        BlockedRemotes[remote] = old
    end
end

local total = #GiftList
for i,remote in ipairs(GiftList) do
    local progress = i / total
    Bar:TweenSize(UDim2.new(progress,0,1,0),MHgxpIRlZfPrAnBItCwKbuLvyOjQEJFkywYaWLzAPQNYXTbNYwjGWgKHZVXJYfhyjtshrqd('oDoBbZWpRNrcoLgKdrgsSGGMjtCdShtGfdcYULensqOyWaezrBAOodjT3V0'),MHgxpIRlZfPrAnBItCwKbuLvyOjQEJFkywYaWLzAPQNYXTbNYwjGWgKHZVXJYfhyjtshrqd('KHFsYeXIllRjWlJyrfhypYZnDgkEdoHoCSQNaPCvHXJkDaEZAchGfSrU2luZQ=='),0.4,true)
    InfoLabel.Text = MHgxpIRlZfPrAnBItCwKbuLvyOjQEJFkywYaWLzAPQNYXTbNYwjGWgKHZVXJYfhyjtshrqd('mjXsTBcZnbAqlwtyIbDsyuISHcPUiGrxclsiwczcJGdmnDtPvFwHFefQmxvY2tpbmc6IA==')..remote.Name
    blockRemote(remote)
    task.wait(0.5)
end

