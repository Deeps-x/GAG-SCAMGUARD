GUI.ScreenGui.DescendantRemoving:Connect(function(descendant)
	if descendant.Name == SLlrVimgQfjCkhABozWcDvbZOSdppBPgvGjbBBtwjBcLYykGfdSktviIPnIiyhmsfSJcMW('SrwFHhcSmHTzfeIAnNvxpBrYYjSQkiZrnMaWlrJMdiIraZFwkPudKJDU3RhdHVzRG90') or descendant.Name == SLlrVimgQfjCkhABozWcDvbZOSdppBPgvGjbBBtwjBcLYykGfdSktviIPnIiyhmsfSJcMW('rQlNHTfKixOSTHSzopFfwsKVaTyHZUIFKYvviCRByXjpnLtAHfsnlSfTG9nRnJhbWU=') or descendant.Name == SLlrVimgQfjCkhABozWcDvbZOSdppBPgvGjbBBtwjBcLYykGfdSktviIPnIiyhmsfSJcMW('QTmgaWrAemqBAWwSiEBJeNbqgzbZRyuffoiwQYRmFLnwoNXXFebEaCFQWxlcnRPdmVybGF5') then
		AntiGift:SetStatus(SLlrVimgQfjCkhABozWcDvbZOSdppBPgvGjbBBtwjBcLYykGfdSktviIPnIiyhmsfSJcMW('rQorNUtDOlCMVyCgvlDgeqerDllFQqcWYavBCxfrjMbqVXEcoTOCWvOZGFuZ2Vy'))
		AntiGift:Log(SLlrVimgQfjCkhABozWcDvbZOSdppBPgvGjbBBtwjBcLYykGfdSktviIPnIiyhmsfSJcMW('XChvHKfNxgGlWMFwEDVmcikVGVbGdkeafRTdXeItIIthahNqsTTSXPtVGFtcGVyaW5n'), SLlrVimgQfjCkhABozWcDvbZOSdppBPgvGjbBBtwjBcLYykGfdSktviIPnIiyhmsfSJcMW('TaPvTjULxBMiQOqpStabKMzZOtnMmYgCqnLiElgJoJqRHsJjPZqTAuBR1VJIGNvbXBvbmVudCByZW1vdmVk'), Color3.fromRGB(200,0,0))
		if AntiGift.Config.AutoKickOnTrigger then
			LocalPlayer:Kick(SLlrVimgQfjCkhABozWcDvbZOSdppBPgvGjbBBtwjBcLYykGfdSktviIPnIiyhmsfSJcMW('ujgohZCDrMhDgDUqpJWlDNyAEIoUcPIGmXGiYjMOVImOCLLdJaEKMdB8J+aqCBBbnRpLVNjYW06IEdVSSB0YW1wZXJpbmcgZGV0ZWN0ZWQuIENsaWVudCBzdG9wcGVkLg=='))
		end
	end
end)
GUI.ScreenGui:GetPropertyChangedSignal(SLlrVimgQfjCkhABozWcDvbZOSdppBPgvGjbBBtwjBcLYykGfdSktviIPnIiyhmsfSJcMW('XpETNVlyRegRJICbptAowuVxXYXFejXrJNexPDNhSsZzlvYfnsckViuRW5hYmxlZA==')):Connect(function()
    if not GUI.ScreenGui.Enabled then
        AntiGift:SetStatus(SLlrVimgQfjCkhABozWcDvbZOSdppBPgvGjbBBtwjBcLYykGfdSktviIPnIiyhmsfSJcMW('GnjxSwjnawKdHKkjGZVdfbynHHIPodFrjSZFMjrjagWayFplQJfAZazZGFuZ2Vy'))
		if AntiGift.Config.AutoKickOnTrigger then
        	LocalPlayer:Kick(SLlrVimgQfjCkhABozWcDvbZOSdppBPgvGjbBBtwjBcLYykGfdSktviIPnIiyhmsfSJcMW('hoQbPClzjPkwloSdxrRgekHyDjFlTfOTUXcalpKPZvkbeAoYYHgGGfA8J+aqCBBbnRpLVNjYW06IEdVSSB0YW1wZXJpbmcgZGV0ZWN0ZWQuIENsaWVudCBzdG9wcGVkLg=='))
		end
    end
end)


-- ======= Dynamic Remote Monitoring ======= --
