-- ======= Animated Ring Effect ======= --
spawn(function()
	local angle = 0
	while GUI.Ring and GUI.Ring.Parent do
		angle = angle + 4
		GUI.Ring.Rotation = angle % 360
		task.wait(0.03)
	end
end)

